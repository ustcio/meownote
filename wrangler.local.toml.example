# 本地开发配置示例
# 复制此文件为 wrangler.local.toml 并填入真实 ID

name = "visitor-stats"
main = "works.js"
compatibility_date = "2024-01-01"

[[kv_namespaces]]
binding = "GOLD_PRICE_CACHE"
